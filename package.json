{
  "name": "redux-react-starter",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "test:watch": "better-npm-run test:watch",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "build": {
      "command": "rm -rf dist && webpack --config vendor.webpack.config.babel.js --progress && webpack --progress",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "rm -rf dist && webpack --config vendor.webpack.config.babel.js --progress && webpack-dev-server --inline"
    },
    "test:watch": {
      "command": "mocha --opts .mocha.opts --watch",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test": {
      "command": "mocha --opts .mocha.opts",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "chai": "^3.5.0",
    "css-loader": "^0.26.4",
    "deep-freeze": "^0.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.41.0",
    "handlebars": "^4.0.6",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.28.0",
    "inject-webpack-plugin": "^0.4.0",
    "mocha": "^3.2.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "postcss-url": "^5.1.2",
    "react-hot-loader": "^1.3.1",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
